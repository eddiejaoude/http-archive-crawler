{
    "name": "eddiejaoude/http-archive-crawler",
    "description": "Crawler powered by HTTPArchive & Web Page Test",
    "license": "BSD-3-Clause",
    "keywords": [
        "Performanace",
        "Crawler",
        "Http Archive",
        "Web Page Test"
    ],
    "homepage": "https://github.com/eddiejaoude/http-archive-crawler",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "eddiejaoude/zf2-common": "dev-master",
        "vdb/php-spider": "dev-master",
        "vdb/uri": "dev-master",
        "zendframework/zftool": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "phpunit/phpunit-selenium": ">=1.2",
        "mockery/mockery": "dev-master@dev",
        "phpmd/phpmd": "1.4.*",
        "verkkokauppacom/parallel-phpunit": "dev-master",
        "Element-34/php-webdriver": "v1.9.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/Element-34/php-webdriver.git"
        },
        {
            "type": "vcs",
            "url": "git://github.com/verkkokauppacom/parallel-phpunit.git"
        },
        {
            "type": "vcs",
            "url": "git://github.com/eddiejaoude/zf2-common.git"
        }

    ],
    "ignore": {
        "sebastian/phpcpd": ">=1.4.*"
    }
}